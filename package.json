{
  "name": "find-my-lunch",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "server": "node ace serve --watch",
    "client": "node ace mix:watch",
    "build": "npm run client:build && npm run server:build",
    "server:build": "node ace build --production",
    "client:build": "node ace mix:build --production",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.2.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "@symfony/webpack-encore": "^4.1.1",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "laravel-mix": "^6.0.49",
    "laravel-mix-tailwind": "^0.1.2",
    "pino-pretty": "^9.1.1",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.8.8",
    "@adonisjs/lucid": "^18.2.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.0",
    "@adonisjs/view": "^6.2.0",
    "@geoapify/geocoder-autocomplete": "^1.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "adonis-mix-asset": "^3.0.0",
    "luxon": "^3.0.4",
    "node-fetch": "^3.2.10",
    "pg": "^8.8.0",
    "phc-argon2": "^1.1.3",
    "proxy-addr": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "source-map-support": "^0.5.21"
  }
}
